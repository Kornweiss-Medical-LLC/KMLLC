@article{duijkersOvarianFunctionContraceptive2004,
  title = {Ovarian Function with the Contraceptive Vaginal Ring or an Oral Contraceptive: {{A}} Randomized Study},
  author = {Duijkers, Ingrid J.M. and Klipping, Christine and Verhoeven, Carole H.J. and Dieben, Thom O.M.},
