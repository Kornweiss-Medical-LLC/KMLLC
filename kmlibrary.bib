  shorttitle = {Carotid {{Artery Wall Thickness}} and {{Incident Cardiovascular Events}}},
  author = {Zhang, Yiyi and Guallar, Eliseo and Malhotra, Saurabh and Astor, Brad C. and Polak, Joseph F. and Qiao, Ye and Gomes, Antoinette S. and Herrington, David M. and Sharrett, A. Richey and Bluemke, David A. and Wasserman, Bruce A.},
