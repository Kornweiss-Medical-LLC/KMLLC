@article{steinberg2005,
  title = {Thematic Review Series: The Pathogenesis of Atherosclerosis. {{An}} Interpretive History of the Cholesterol Controversy: Part {{II}}: The Early Evidence Linking Hypercholesterolemia to Coronary Disease in Humans},
