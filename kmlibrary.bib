}

@article{zhangClinicalCharacteristics822020,
  title = {Clinical Characteristics of 82 Death Cases with {{COVID-19}}},
  author = {Zhang, Bicheng and Zhou, Xiaoyang and Qiu, Yanru and Feng, Fan and Feng, Jia and Jia, Yifan and Zhu, Hengcheng and Hu, Ke and Liu, Jiasheng and Liu, Zaiming and Wang, Shihong and Gong, Yiping and Zhou, Chenliang and Zhu, Ting and Cheng, Yanxiang and Liu, Zhichao and Deng, Hongping and Tao, Fenghua and Ren, Yijun and Cheng, Biheng and Gao, Ling and Wu, Xiongfei and Yu, Lilei and Huang, Zhixin and Mao, Zhangfan and Song, Qibin and Zhu, Bo and Wang, Jun},
  year = {2020},
