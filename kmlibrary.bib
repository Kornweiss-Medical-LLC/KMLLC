@article{ebbing2009,
  title = {Cancer {{Incidence}} and {{Mortality After Treatment With Folic Acid}} and {{Vitamin B12}}},
  author = {Ebbing, Marta and B{\o}naa, Kaare Harald and Nyg{\aa}rd, Ottar and Arnesen, Egil and Ueland, Per Magne and Nordrehaug, Jan Erik and Rasmussen, Knut and Nj{\o}lstad, Inger and Refsum, Helga and Nilsen, Dennis W. and Tverdal, Aage and Meyer, Klaus and Vollset, Stein Emil},
