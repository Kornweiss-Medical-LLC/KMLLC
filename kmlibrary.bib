@article{ishibashiHypercholesterolemiaLowDensity1993,
  title = {Hypercholesterolemia in Low Density Lipoprotein Receptor Knockout Mice and Its Reversal by Adenovirus-Mediated Gene Delivery.},
  author = {Ishibashi, S and Brown, M S and Goldstein, J L and Gerard, R D and Hammer, R E and Herz, J},
  year = {1993},
