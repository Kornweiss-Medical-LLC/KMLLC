@article{han2008,
  title = {Sex Differences in Atheroma Burden and Endothelial Function in Patients with Early Coronary Atherosclerosis},
  author = {Han, Seung Hwan and Bae, Jang Ho and Holmes, David R. and Lennon, Ryan J. and Eeckhout, Eric and Barsness, Gregory W. and Rihal, Charanjit S. and Lerman, Amir},
