  author = {Nagano, Satoshi and Yahiro, Yuhei and Yokouchi, Masahiro and Setoguchi, Takao and Ishidou, Yasuhiro and Sasaki, Hiromi and Shimada, Hirofumi and Kawamura, Ichiro and Komiya, Setsuro},
