@article{liLipidRaftsPlay2007,
  title = {Lipid Rafts Play an Important Role in the Early Stage of Severe Acute Respiratory Syndrome-Coronavirus Life Cycle},
  author = {Li, Gui-Mei and Li, Yong-Gang and Yamate, Masanobu and Li, Shu-Ming and Ikuta, Kazuyoshi},
