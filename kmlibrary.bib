@article{songRiskGastrointestinalCancers2016,
  title = {Risk of {{Gastrointestinal Cancers}} among {{Patients}} with {{Appendectomy}}: {{A Large}}-{{Scale Swedish Register}}-{{Based Cohort Study}} during 1970-2009},
  shorttitle = {Risk of {{Gastrointestinal Cancers}} among {{Patients}} with {{Appendectomy}}},
