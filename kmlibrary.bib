@article{tangHydroxychloroquinePatientsMainly2020,
  title = {Hydroxychloroquine in Patients with Mainly Mild to Moderate Coronavirus Disease 2019: Open Label, Randomised Controlled Trial},
  shorttitle = {Hydroxychloroquine in Patients with Mainly Mild to Moderate Coronavirus Disease 2019},
  author = {Tang, Wei and Cao, Zhujun and Han, Mingfeng and Wang, Zhengyan and Chen, Junwen and Sun, Wenjin and Wu, Yaojie and Xiao, Wei and Liu, Shengyong and Chen, Erzhen and Chen, Wei and Wang, Xiongbiao and Yang, Jiuyong and Lin, Jun and Zhao, Qingxia and Yan, Youqin and Xie, Zhibin and Li, Dan and Yang, Yaofeng and Liu, Leshan and Qu, Jieming and Ning, Guang and Shi, Guochao and Xie, Qing},
  year = {2020},
