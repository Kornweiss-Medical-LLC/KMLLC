  pmid = {4360855},
  keywords = {Adipose Tissue,Adult,Biological Transport,Biopsy,Carbon Radioisotopes,Cholesterol,Erythrocytes,Female,Humans,Intestinal Absorption,Lipid Metabolism; Inborn Errors,Lipoproteins; HDL,Lipoproteins; LDL,Lipoproteins; VLDL,Sitosterols,Skin,Tendons,Xanthomatosis},
