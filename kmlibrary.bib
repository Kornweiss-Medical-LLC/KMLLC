  langid = {english},
  pmid = {32409561},
  file = {/Users/sk/Zotero/storage/2G8KX2H4/Tang et al. - 2020 - Hydroxychloroquine in patients with mainly mild to.pdf;/Users/sk/Zotero/storage/UU52HAUG/bmj.m1849.html}
