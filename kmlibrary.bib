@article{goodman1983,
  title = {Cholesterol Turnover and Metabolism in Two Patients with Abetalipoproteinemia.},
  author = {Goodman, D S and Deckelbaum, R J and Palmer, R H and Dell, R B and Ramakrishnan, R and Delpre, G and Beigel, Y and Cooper, M},
