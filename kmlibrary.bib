  shorttitle = {Early and {{Critical Care}} in {{Severe Patients}} with {{COVID-19}} in {{Jiangsu Province}}, {{China}}},
  author = {Huang, Mao and Yang, Yi and Shang, Futai and Zheng, Yishan and Zhao, Wenjing and Luo, Liang and Han, Xudong and Lin, Aihua and Zhao, Hongsheng and Gu, Qing and Shi, Yi and Li, Jun and Xu, Xingxiang and Liu, Kexi and Deng, YiJun and Cao, Quan and Wang, Weiwei},
  year = {2020},
