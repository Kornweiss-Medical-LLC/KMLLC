@article{ji2008,
  title = {Rare Independent Mutations in Renal Salt Handling Genes Contribute to Blood Pressure Variation},
  author = {Ji, Weizhen and Foo, Jia Nee and O'Roak, Brian J. and Zhao, Hongyu and Larson, Martin G. and Simon, David B. and {Newton-Cheh}, Christopher and State, Matthew W. and Levy, Daniel and Lifton, Richard P.},
  year = {2008},
