@article{pearson1997,
  title = {Age-{{Associated Changes}} in {{Blood Pressure}} in a {{Longitudinal Study}} of {{Healthy Men}} and {{Women}}},
  author = {Pearson, Jay D. and Morrell, Christopher H. and Brant, Larry J. and Landis, Patricia K. and Fleg, Jerome L.},
  year = {1997},
