@article{kreider2017,
  title = {International {{Society}} of {{Sports Nutrition}} Position Stand: Safety and Efficacy of Creatine Supplementation in Exercise, Sport, and Medicine},
  shorttitle = {International {{Society}} of {{Sports Nutrition}} Position Stand},
  author = {Kreider, Richard B. and Kalman, Douglas S. and Antonio, Jose and Ziegenfuss, Tim N. and Wildman, Robert and Collins, Rick and Candow, Darren G. and Kleiner, Susan M. and Almada, Anthony L. and Lopez, Hector L.},
  year = {2017},
