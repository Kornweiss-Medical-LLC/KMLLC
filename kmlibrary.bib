  author = {Wang, Zaimin and Knight, Sabina and Wilson, Andrew and Rowley, Kevin G. and Best, James D. and McDermott, Robyn and Leonard, Dympna and Shaw, Jonathan E. and O'Dea, Kerin},
  year = {2006},
