  shorttitle = {Fluvoxamine vs {{Placebo}} and {{Clinical Deterioration}} in {{Outpatients With Symptomatic COVID-19}}},
  author = {Lenze, Eric J. and Mattar, Caline and Zorumski, Charles F. and Stevens, Angela and Schweiger, Julie and Nicol, Ginger E. and Miller, J. Philip and Yang, Lei and Yingling, Michael and Avidan, Michael S. and Reiersen, Angela M.},
