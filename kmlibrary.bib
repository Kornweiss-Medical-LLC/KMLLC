@article{stampfer1992,
  title = {A {{Prospective Study}} of {{Plasma Homocyst}}(e)Ine and {{Risk}} of {{Myocardial Infarction}} in {{US Physicians}}},
  author = {Stampfer, Meir J. and Malinow, M. Rene and Willett, Walter C. and Newcomer, Laura M. and Upson, Barbara and Ullmann, Daniel and Tishler, Peter V. and Hennekens, Charles H.},
  year = {1992},
