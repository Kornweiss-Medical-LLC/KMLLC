  author = {Kramer, Holly and Colangelo, Laura and Lewis, Cora E. and Jacobs, Jr, David R. and Pletcher, Mark and {Bibbins-Domingo}, Kirstin and Chang, Alex and Siscovick, David and Shlipak, Michael and Peralta, Carmen A. and Bansal, Nisha and Muntner, Paul and Liu, Kiang},
